{block content}
	<h1>Správa knihoven</p>
	{if $user->isAllowed('AdminPage')}<li><a n:href=":Library:LibraryAdd:">Přidat knihovnu</a></li>{/if}
	{if $knihovny}
		<ol>
		{foreach $knihovny as $knihovna}
			<div class="knihovnaItem">
				<li><a href="{link Library: $knihovna->ID}">{$knihovna->ID}</a></li>
			</div>
		{/foreach}
		</ol>
	{/if}

{/block}